<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.github.server.GreetingServiceImpl</servlet-class>
  </servlet>
    <servlet>
        <servlet-name>com.github.GwtTest DateCheckerService</servlet-name>
        <servlet-class>com.github.server.DateCheckerImpl</servlet-class>
    </servlet>

    <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/GwtTest/greet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.github.GwtTest DateCheckerService</servlet-name>
        <url-pattern>/GwtTest/DateCheckerService</url-pattern>
    </servlet-mapping>

    <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>GwtTest.html</welcome-file>
  </welcome-file-list>

</web-app>
